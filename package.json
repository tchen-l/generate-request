{
  "name": "generate-request",
  "displayName": "generate-request",
  "description": "语忆前端自动生成接口代码",
  "main": "./extension.ts",
  "version": "0.0.7",
  "publisher": "tchen",
  "repository": {
    "url": "git@github.com:895433995/generate-request.git"
  },
  "icon": "logo.png",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p -w . --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:generate-request.interfaceCode"
  ],
  "contributes": {
    "commands": [
      {
        "command": "generate-request.interfaceCode",
        "title": "Interface Code"
      }
    ],
    "configuration": [
      {
        "type": "object",
        "title": "generate-request",
        "properties": {
          "generateRequest.username": {
            "type": "string",
            "default": "",
            "description": "账号"
          },
          "generateRequest.password": {
            "type": "string",
            "default": "",
            "description": "密码"
          },
          "generateRequest.cookie": {
            "type": "string",
            "default": "",
            "description": "如果填写了cookie，账号密码可不填"
          }
        }
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/md5": "^2.3.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/qs": "^6.9.7",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "md5": "^2.3.0",
    "qs": "^6.10.1"
  }
}
