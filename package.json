{
  "name": "generate-request",
  "displayName": "generate-request",
  "description": "语忆前端自动生成接口代码",
  "main": "./extension.js",
  "version": "0.0.7",
  "publisher": "tchen",
  "repository": {
    "url": "git@github.com:895433995/generate-request.git"
  },
  "icon": "logo.png",
  "scripts": {
    "publish-vsce": "vsce publish",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:generate-request.interfaceCode"
  ],
  "contributes": {
    "commands": [
      {
        "command": "generate-request.interfaceCode",
        "title": "Interface Code"
      }
    ],
    "configuration": [
      {
        "type": "object",
        "title": "generate-request",
        "properties": {
          "generateRequest.username": {
            "type": "string",
            "default": "",
            "description": "账号"
          },
          "generateRequest.password": {
            "type": "string",
            "default": "",
            "description": "密码"
          },
          "generateRequest.cookie": {
            "type": "string",
            "default": "",
            "description": "如果填写了cookie，账号密码可不填"
          }
        }
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.59.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3",
    "@vscode/test-electron": "^1.6.2"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "md5": "^2.3.0",
    "qs": "^6.10.1"
  }
}