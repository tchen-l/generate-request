{
  "name": "generate-request",
  "displayName": "generate-request",
  "description": "语忆前端自动生成接口代码",
  "main": "./out/extension.js",
  "version": "0.0.10",
  "publisher": "tchen",
  "repository": {
    "url": "git@github.com:895433995/generate-request.git"
  },
  "icon": "logo.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "prepare": "husky install"
  },
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:generate-request.interfaceCode"
  ],
  "contributes": {
    "commands": [
      {
        "command": "generate-request.interfaceCode",
        "title": "Interface Code"
      }
    ],
    "configuration": [
      {
        "type": "object",
        "title": "generate-request",
        "properties": {
          "generateRequest.username": {
            "type": "string",
            "default": "",
            "description": "账号"
          },
          "generateRequest.password": {
            "type": "string",
            "default": "",
            "description": "密码"
          },
          "generateRequest.cookie": {
            "type": "string",
            "default": "",
            "description": "如果填写了cookie，账号密码可不填"
          }
        }
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/md5": "^2.3.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/qs": "^6.9.7",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "glob": "^7.1.7",
    "husky": "^8.0.1",
    "mocha": "^9.1.1",
    "prettier": "^2.7.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "md5": "^2.3.0",
    "qs": "^6.10.1"
  }
}
